#include <stdio.h>

int sstrlen(const char * arr){
    int len = 0;
    for(len = 0; arr[len] != 0; ++len);
    return len;
}

int ggetline(FILE* in, char *buffer){
    char c;
    int i = 0;
    while ((c = fgetc(in)) != EOF && c != '\n'){
        buffer[i] = c;
        ++i;
    }
    buffer[i] = 0;
    if (c == EOF){
        return 0;
    }
    return 1;
}


int main(){
    FILE* in;
    in = fopen("input.txt", "r");
    char line[64];
    char *ln = line;
    while(ggetline(in, ln)){
        for(int i = 0; i < sstrlen(ln) / 2; ++i){
            char tmp = ln[i];
            ln[i] = ln[strlen(ln) - 1 - i];
            ln[strlen(ln) - 1 - i] = tmp;
        }
        printf("%s\n", line);
    }
    fclose(in);
}

