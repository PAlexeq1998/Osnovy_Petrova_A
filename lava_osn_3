#include <stdio.h>

int ggetline(FILE* in, char *buffer){
    char c;
    int i = 0;
    while ((c = fgetc(in)) != EOF && c != '\n'){
        buffer[i] = c;
        ++i;
    }
    buffer[i] = 0;
    if (c == EOF){
        return 0;
    }
    return 1;
}


int main(){
    FILE* in;
    in = fopen("input.txt", "r");
    char line[10][64];
    int size = 0;
    while(ggetline(in, line[size])){
        ++size;
    }
    while(size >= 0){
        printf("%s\n", line[size]);
        --size;
    }
    fclose(in);
}
